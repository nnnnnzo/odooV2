<odoo>
    <record model="ir.ui.view" id="students_student_view_form">
        <field name="name">students.form</field>
        <field name="model">students.student</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Student">
                <sheet>
                    <group>
                        <group>
                            <field name="number"/>
                            <field name="training_id"/>
                            <field name="avgMark"/>
                        </group>
                        <group>
                            <field name="firstname"/>
                            <field name="lastname"/>
                            <field name="country"/>
                        </group>

                    </group>
                    <notebook>
                        <page string="Marks" name="mark">
                            <field name="mark_ids">
                                <tree editable="bottom">
                                    <field name="subject"/>
                                    <field name="mark" />
                                    <field name="coefficient"/>
                                    <field name="coefMark"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="students_continious_view_form">
        <field name="name">studentcontinuous.form</field>
        <field name="model">students.studentcontinuous</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Student">
                <sheet>
                    <group>
                        <group>
                            <field name="number"/>
                            <field name="training_id"/>
                            <field name="avgMark"/>
                        </group>
                        <group>
                            <field name="firstname"/>
                            <field name="lastname"/>
                            <field name="country"/>
                            <field name="partner"/>
                        </group>

                    </group>
                    <notebook>
                        <page string="Marks" name="mark">
                            <field name="mark_ids">
                                <tree editable="bottom">
                                    <field name="subject"/>
                                    <field name="mark" />
                                    <field name="coefficient"/>
                                    <field name="coefMark"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="students_training_view_form">
        <field name="name">training.form</field>
        <field name="model">students.training</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Training">
                <sheet>
                    <group string="Information">
                        <field name="code"/>
                        <field name="name"/>
                    </group>
                    <notebook>
                        <page string="Students" name="student">
                            <field name="student_ids">
                                <tree editable="bottom">
                                    <field name="number"/>
                                    <field name="firstname"/>
                                    <field name="lastname"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="students_mark_view_form">
        <field name="name">marks.form</field>
        <field name="model">students.mark</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Marks">
                <sheet>
                    <group>
                        <field name="subject"/>
                        <field name="mark" />
                        <field name="coefficient"/>
                        <field name="coefMark"/>
                        <field name="student_ids"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="students_student_view_tree">
        <field name="name">students.tree</field>
        <field name="model">students.student</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Students">
                <field name="firstname"/>
                <field name="lastname"/>
                <field name="avgMark"/>
                <field name="training_id"/>
                <field name="mark_ids"/>
            </tree>
        </field>
    </record>
    <record model="ir.ui.view" id="students_continious_view_tree">
        <field name="name">studentcontinuous.tree</field>
        <field name="model">students.studentcontinuous</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Students">
                <field name="firstname"/>
                <field name="lastname"/>
                <field name="avgMark"/>
                <field name="training_id"/>
                <field name="partner"/>
                <field name="mark_ids"/>
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="students_training_view_tree">
        <field name="name">trainings.tree</field>
        <field name="model">students.training</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Trainings">
                <field name="code"/>
                <field name="name"/>
                <field name="student_ids"/>
            </tree>
        </field>
    </record>
    <record model="ir.ui.view" id="students_mark_view_tree">
        <field name="name">mark.tree</field>
        <field name="model">students.mark</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Marks">
                <field name="subject"/>
                <field name="mark" />
                <field name="coefficient"/>
                <field name="coefMark"/>
                <field name="student_ids"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_students_student_view">
        <field name="name">Students</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">students.student</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="action_students_continious_view">
        <field name="name">Students Continious</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">students.studentcontinuous</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="action_trainings_student_view">
        <field name="name">Trainings</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">students.training</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="action_marks_student_view">
        <field name="name">Marks</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">students.mark</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="students_menu" name="Students"/>
    <menuitem id="students_training_menu" name="Trainings" parent="students_menu"
              action="action_trainings_student_view"/>
    <menuitem id="students_student_menu" name="Students" parent="students_menu"/>
    <menuitem id="students_student_student_menu" name="Students" parent="students_student_menu" action="action_students_student_view"/>
    <menuitem id="students_mark_menu" name="Marks" parent="students_menu"
              action="action_marks_student_view"/>

    <menuitem id="students_continious_menu" name="Students Continious" parent="students_student_menu" action="action_students_continious_view"/>
</odoo>